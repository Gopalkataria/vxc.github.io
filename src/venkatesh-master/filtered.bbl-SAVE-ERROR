    \entry{Choppella-2004-compositionality}{unpublished}{}
      \name{author}{1}{}{%
        {{hash=734f94cfff2a88f90d5a713df69b578d}{%
           family={Choppella},
           familyi={C\bibinitperiod},
           given={Venkatesh},
