    \entry{choppella-sengupta-robertson-johnson-tr-2006}{report}{}
      \name{author}{4}{}{%
        {{hash=734f94cfff2a88f90d5a713df69b578d}{%
           family={Choppella},
           familyi={C\bibinitperiod},
           given={Venkatesh},
           giveni={V\bibinitperiod}}}%
        {{hash=24badadc9b886cead215cb4c27d705fa}{%
           family={Sengupta},
