      \name{editor}{1}{}{%
        {{hash=7bb2fce7a444c4ce91a7e2a9d8a0336e}{%
           family={al.},
           familyi={a\bibinitperiod},
           given={V.\bibnamedelimi N.\bibnamedelimi Alexandrov},
